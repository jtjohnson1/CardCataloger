# Docker Environment Configuration
# Copy this file to .env and update with your actual values

# JWT Secrets (REQUIRED - Change these in production!)
JWT_SECRET=your_super_secure_jwt_secret_here_change_in_production
REFRESH_TOKEN_SECRET=your_super_secure_refresh_secret_here_change_in_production

# eBay API Configuration (Optional - for price comparisons)
EBAY_APP_ID=your_ebay_app_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_USER_TOKEN=your_ebay_user_token_here
EBAY_SANDBOX=true

# Database Configuration (Auto-configured for Docker)
DATABASE_URL=mongodb://mongodb:27017/CardCataloger

# Ollama Configuration (Auto-configured for Docker)
OLLAMA_BASE_URL=http://ollama:11434